{
	"name": "algo-mate",
	"displayName": "Algo-Mate",
	"description": "Algo-Mate extension. Learn more at: https://alexbochman.github.io/ClassesAndProjects/SW-Eng/Algo-MateProject/Algo-Mate.html",
	"version": "0.0.3",
	"publisher": "AlexBochman",
	"repository": {
		"type": "git",
		"url": "https://github.com/alexbochman/Algo-Mate"
	},
	"icon": "Algo-Mate.png",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:algo-mate.helloWorld"
	],
	"main": "./src/user-interface.js",
	"contributes": {
		"commands": [
			{
				"command": "algo-mate.helloWorld",
				"title": "Hello World"
			}
		],
		"snippets": [
			{
				"language": "cpp",
				"path": "./src/cpp.json"
			},
			{
				"language": "javascript",
				"path": "./src/javascript.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "jest"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.54.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"jest": "^26.6.3",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vsce": "^1.87.0",
		"vscode-test": "^1.5.0"
	},
	"keywords": [
		"snippet",
		"cpp",
		"java",
		"javascript",
		"python",
		"code"
	],
	"dependencies": {
		"@types/jest": "^26.0.21"
	}
}
